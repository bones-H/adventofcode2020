 //part one answer 230

const slope = [
  {right: 1, down: 1},
  {right: 3, down: 1},
  {right: 5, down: 1},
  {right: 7, down: 1},
  {right: 1, down: 2},
]
let AnswerArr = [];
slope.forEach(route => {
  let trees = 0;
  let coordinates = {
    column: 0,
    line: 0,
  };
  result.forEach( (item, index) => {
    if (index === coordinates.line) {
      if (item[coordinates.column] === '#') {
        trees++;
      }
      coordinates.column += route.right;
      if(coordinates.column > item.length - 1) {
        coordinates.column = coordinates.column - item.length
      }
      coordinates.line += route.down
    }
  })
  AnswerArr.push(trees)
})
console.log(AnswerArr.reduce((result, current) => result * current))
// answer part 2 9533698720

