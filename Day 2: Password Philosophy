part one 

let result = str.split('\n').map(a => a.split(' ')).map(  item => {
  const [min, max] = item[0].split('-');
  const letter = item[1][0];
  const str = item[2];
  return {
    key: letter,
    password: str,
    min: min,
    max: max,
  }
  }
)
part one answer 378
const validPasswords = result.filter(item => {
  const letterMatches = item.password.split('').filter( letter => letter === item.key);
  return letterMatches.length >= item.min && letterMatches.length <= item.max
})
